        <fullName>Validar_campo_Mes_Ano</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
    NOT(ISBLANK(TW1_Mes_Ano__c)), 
    IF(NOT(REGEX(TW1_Mes_Ano__c,&quot;01/[0-9]{2}/[0-9]{4}&quot;)), 
       true, 
       OR(CASE(MID(TW1_Mes_Ano__c,4,2), 
          &quot;01&quot;,1, 
          &quot;02&quot;,1, 
          &quot;03&quot;,1, 
          &quot;04&quot;,1, 
          &quot;05&quot;,1, 
          &quot;06&quot;,1, 
          &quot;07&quot;,1, 
          &quot;08&quot;,1, 
          &quot;09&quot;,1, 
          &quot;10&quot;,1, 
          &quot;11&quot;,1, 
          &quot;12&quot;,1, 
            0)=0, 
          VALUE(RIGHT(TW1_Mes_Ano__c,4))&lt; 1900)) 

   )</errorConditionFormula>
        <errorDisplayField>TW1_Mes_Ano__c</errorDisplayField>
        <errorMessage>Campo deve ser no formato: ex: 01/MES/ANO.
Mês - 01 a 12 
Ano - 1900 - atual</errorMessage>
    </validationRules>
