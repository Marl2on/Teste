    <validationRules>
        <fullName>Tw1_Validacao_Dados_Celular</fullName>
        <active>true</active>
        <description>Valida quando o tipo selecionado for celular</description>
        <errorConditionFormula>IF ($RecordType.Name = &quot;Celular&quot;, NOT(
OR(
ISBLANK( Tw1_Contato__c ),
REGEX( Tw1_Contato__c ,&quot;\\([0-9]{2}\\)[0-9]{4,5}-[0-9]{4}&quot;)
)
),FALSE)</errorConditionFormula>
        <errorMessage>Verifique se o contato está escrito no formato  (xx)xxxxx-xxxx.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Tw1_Validacao_Dados_Email</fullName>
        <active>true</active>
        <description>Valida quando o tipo digitado é e-mail</description>
        <errorConditionFormula>IF ( RecordType.DeveloperName = &quot;Tw1_RT_E_mail&quot;, NOT(REGEX( Tw1_Contato__c ,&apos;([a-zA-Z0-9_\\-\\.]+)@((\\[a-z]{1,3}\\.[a-z]{1,3}\\.[a-z]{1,3}\\.)|(([a-zA-Z0-9\\-]+\\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})&apos;)), FALSE)</errorConditionFormula>
        <errorMessage>Verifique se o e-mail cadastrado segue o padrão contato@dominio.tipo</errorMessage>
    </validationRules>
