    <validationRules>
        <fullName>TW1_Solicita_dados_cliente</fullName>
        <active>false</active>
        <errorConditionFormula>ISCHANGED(TW1_Status__c) &amp;&amp; TEXT(TW1_Status__c) = &quot;Devolvido&quot; &amp;&amp; (ISBLANK(TW1_Cliente__c) || ISBLANK(TW1_Data_da_Devolucao__c))</errorConditionFormula>
        <errorMessage>Por favor preencha os dados da conta e a data da devolução.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TW1_Solicita_dados_doacao</fullName>
        <active>false</active>
        <errorConditionFormula>ISCHANGED(TW1_Status__c) &amp;&amp; ISPICKVAL(TW1_Status__c, &quot;Doado&quot;) &amp;&amp; (ISBLANK(TW1_Empresa_Destino__c) || ISBLANK(TW1_Data_da_Doacao__c))</errorConditionFormula>
        <errorMessage>Caso o status seja alterado para Doado se faz necessário os dados da doação nos campos: Empresa Destino e Data da Doação antes de salvar.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TW1_WF_AchadoPerdido_datadevolucaonotbla</fullName>
        <active>true</active>
        <description>Se o campo Status estiver como devolvido, o campo data da devolução não pode estar em branco.</description>
        <errorConditionFormula>AND( 
ISPICKVAL( TW1_Status__c ,&quot;Devolvido&quot;), 
ISBLANK( TW1_Data_da_Devolucao__c ) 
)</errorConditionFormula>
        <errorDisplayField>TW1_Data_da_Devolucao__c</errorDisplayField>
        <errorMessage>Favor preencher o campo ´´ Data da Devolução ´´ quando o Status estiver como Devolvido.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TW1_WF_AchadoPerdido_datadoacaonotblank</fullName>
        <active>true</active>
        <description>Se o campo Status estiver como Doado, a data da doação não pode estar em branco.</description>
        <errorConditionFormula>AND( 

ISPICKVAL(TW1_Status__c, &quot;Doado&quot;), ISBLANK(TW1_Data_da_Doacao__c))</errorConditionFormula>
        <errorDisplayField>TW1_Data_da_Doacao__c</errorDisplayField>
        <errorMessage>Favor preencher o campo ´´ Data da Doação ´´ quando o Status estiver como Doado</errorMessage>
    </validationRules>
